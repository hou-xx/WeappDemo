.btn-outer{
   display: flex;
  flex-direction: raw;
  justify-content: space-around;
}
.btn-raw-screen{
    flex: 1;
    text-align: center;
    margin-right: 16rpx;
    padding: 16rpx;
    border: 1rpx solid #37BC9B;
    border-radius: 10rpx;
    color: #37BC9B;
