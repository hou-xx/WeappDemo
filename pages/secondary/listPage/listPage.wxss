.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.textStyle {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.textContainer {
  margin-top: 200px;
}
.list-header{
  font-size: 32rpx;
  text-align: center;
  margin-bottom:16rpx;
   padding: 16rpx;
    border: 1rpx solid #37BC9B;
    border-radius: 10rpx;
    color: #37BC9B;
}
.item-outer{
   display: flex;
  flex-direction: raw;
  justify-content: space-around;
  padding: 16rpx 0;
  border-top: 1rpx solid #48CFAD;
}
.item-outer:last-child{
   border-bottom: 1rpx solid #48CFAD;
}
.item-left{
  flex: 1;
    text-align: center;
    font-size: 28rpx;
    color: #353535;

}
.item-right{
    flex: 3;
    font-size: 28rpx;
    color: #353535;
}
.loading-view {
  display: flex;
  flex-flow: raw wrap;
  justify-content: center;
  margin-bottom: 22rpx;
  height: 30rpx;
  border-top: 1rpx solid #48CFAD;
  padding: 16rpx 0;
}

.loading-icon {
  height: 30rpx;
  width: 30rpx;
  margin-right: 10rpx;
}

.loading-text {
  height: 30rpx;
  line-height: 30rpx;
  color: #b2b2b2;
  font-size: 24rpx;
}
